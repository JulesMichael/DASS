html, body:
	background-color: #ecf0f1;
	font-family: 'Roboto', sans-serif;

.center:
	text-align: center;

.container:
	padding: 20px;

h1 :
	color: #9b59b6;

img :
	margin-top: 40px;
	height: 150px;

.nav :
	background-color: #2c3e50;
	height: 50px;

.title :
	color: #fff;
	font-size: 30px;
	padding: 10px;

function cardUnHover():
	.card:
		box-shadow: 1px 1px 8px 0px rgba(119, 119, 119, 0.75);
		-moz-box-shadow: 1px 1px 8px 0px rgba(119, 119, 119, 0.75);
		-webkit-box-shadow: 1px 1px 8px 0px rgba(119, 119, 119, 0.75);
	.content :
		color: #2c3e50;
cardUnHover();

.card:
	background-color: #fff;
	padding: 10px;
	border-radius: 3px;

&:hover :
	box-shadow: 2px 2px 8px 0px rgba(119, 119, 119, 0.75);
	-moz-box-shadow: 2px 2px 8px 0px rgba(119, 119, 119, 0.75);
	-webkit-box-shadow: 2px 2px 8px 0px rgba(119, 119, 119, 0.75);
	.content :
		color: #2ecc71;
	console.log(getElement(".card"));
	// console.log(tinycolor(getProperty(".card","color")).toHexString());

&:unhover :
	cardUnHover();

.card .title:
	color: #2980b9;
	text-decoration: underline;

.content:
	margin-top: 10px;
