html, body:
    background-color: #ecf0f1;
    font-family: 'Roboto', sans-serif;

.center:
    text-align: center;

.container:
    padding: 20px;

img :
    margin-top: 40px;
    height: 150px;

.nav :
    height: 50px;

.title :
    color: #fff;
    font-size: 30px;
    padding: 10px;

.content:
    margin-top: 10px;

pre :
    // border-top: 1px solid #2c3e50;
    // border-bottom: 1px solid #2c3e50;
    border-left: 5px solid #5A7897;
    // background-color: #E3E6E6;
    padding: 10px;

.masonry :
    // Masonry container
    column-gap: 1em;


function column():
    $width = document.body.clientWidth;
    .masonry :
        column-count: 1;
    if width >= 601:
        .masonry :
            column-count: 2;
    if width >= 993:
        .masonry :
            column-count: 4;

.item :
    // Masonry bricks or child elements
    //border: solid 1px #9b59b6;
    background-color: #fff;
    display: inline-block;
    margin: 0 0 1em;
    width: 100%;
    box-shadow: 2px 2px 3px 1px rgba(119, 119, 119, 0.5);
    -moz-box-shadow: 2px 2px 3px 1px rgba(119, 119, 119, 0.5);
    -webkit-box-shadow: 2px 2px 3px 1px rgba(119, 119, 119, 0.5);

$themes = [{"title": "#1abc9c","nav":"#16a085"},{"title": "#f1c40f","nav":"#f39c12"},{"title": "#2ecc71","nav":"#27ae60"},{"title": "#e67e22","nav":"#d35400"},{"title": "#3498db","nav":"#2980b9"},{"title": "#e74c3c","nav":"#c0392b"},{"title": "#9b59b6","nav":"#8e44ad"},{"title": "#ecf0f1","nav":"#bdc3c7"},{"title": "#34495e","nav":"#2c3e50"},{"title": "#95a5a6","nav":"#7f8c8d"}];

function changeColor ():
    $rand = themes[Math.floor(Math.random() * themes.length)];
    style("h1, h2, h3, .btn","color",rand["title"]);
    style(".nav","background-color",rand["nav"]);
    console.log(rand);

changeColor();
column();